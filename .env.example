# ============================================
# 환경변수 예시 파일
# 이 파일을 복사하여 .env 파일을 생성하세요
# cp .env.example .env
# ============================================

# 거래 모드 (paper: 모의투자, real: 실전투자)
TRADING_MODE=paper

# ============================================
# 한국투자증권 Open API
# https://apiportal.koreainvestment.com/
# ============================================

# 실전투자 계좌
real_account_api_key=YOUR_REAL_API_KEY
real_account_api_secret=YOUR_REAL_API_SECRET
real_account_number=YOUR_REAL_ACCOUNT_NUMBER
real_account_product=01

# 모의투자 계좌
fake_account_api_key=YOUR_FAKE_API_KEY
fake_account_api_secret=YOUR_FAKE_API_SECRET
fake_account_number=YOUR_FAKE_ACCOUNT_NUMBER
fake_account_product=01

# ============================================
# OpenAI API
# https://platform.openai.com/
# ============================================
openai_api_key=YOUR_OPENAI_API_KEY

# ============================================
# Discord
# https://discord.com/developers/applications
# ============================================
DISCORD_BOT_TOKEN=YOUR_DISCORD_BOT_TOKEN
DISCORD_WEBHOOK_URL=YOUR_DISCORD_WEBHOOK_URL
